<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="web" verbose="2">
  <parameter name="xml.report" value="true"/>
  <parameter name="visual.report" value="1"/>
  <parameter name="ats.log.level" value="error"/>
  <test name="testMain" preserve-order="true">
    <packages>
      <package name=".*">
        <exclude name="api.*"/>
        <exclude name="desktop.*"/>
        <exclude name="manual.*"/>
        <exclude name="mobile.*"/>
        <exclude name="subscripts.*"/>
      </package>
    </packages>
    <classes>
      <class name="web.all.exec"/>
    </classes>
  </test>
</suite>