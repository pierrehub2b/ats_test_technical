<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="web" verbose="2">
  <parameter name="xml.report" value="true"/>
  <parameter name="visual.report" value="1"/>
  <parameter name="ats.log.level" value="error"/>
  <test name="testMain" preserve-order="true">
    <classes>
      <class name="web.all.execChrome"/>
      <class name="web.all.execFirefox"/>
      <class name="web.all.execIE"/>
      <class name="web.all.execMsEdge"/>
      <class name="web.all.execOpera"/>
    </classes>
  </test>
</suite>